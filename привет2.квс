.глобал _start

.данные
сообщение: .строка_нуль "Привет, мир!\n"

.текст
_start:
    ; sys_write(1, сообщение, 14)
    переместить_имм рвх, 1      ; rax = 1 → sys_write
    переместить_имм рди, 1      ; rdi = fd (stdout)
    загрузить_адрес рис, сообщение ; rsi = буфер
    переместить_имм рдх, 22     ; 
    вызов_системы

    ; sys_exit(0)
    переместить_имм рвх, 60     ; rax = 60 → sys_exit
    переместить_имм рди, 0      ; rdi = код возврата
    вызов_системы
