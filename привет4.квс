.данные
сообщение:
.строка_нуль "Привет из LEA!\n"

.текст
.глобал _start
_start:
    переместить_имм рди, 1          ; fd = stdout (1)
    загрузить_адрес рис, сообщение  ; buf = адрес строки
    переместить_имм рдх, 23         ; count = 19
    переместить_имм рвх, 1          ; sys_write
    вызов_системы

    переместить_имм рвх, 60         ; sys_exit
    переместить_имм рди, 0          ; status = 0
    вызов_системы