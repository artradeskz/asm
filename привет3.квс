    .глобал _start
    .текст
    _start:
        переместить_имм рвх, 1
        переместить_имм рди, 1
        переместить_имм рис, привет_мир
        переместить_имм рдх, длина_строки
        ; Допустим, "вызов_системы" ещё не реализована — вставим вручную:
        .байт 0x0F 0x05
        переместить_имм рвх, 60
        переместить_имм рди, 0
        .байт 0x0F 0x05

    .данные
    привет_мир:
        .строка "Привет мир!\n"
    .константа длина_строки 23