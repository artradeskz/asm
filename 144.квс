.глобал _start

.данные
текст: .строка_нуль "144\n"

.текст
_start:
    ; --- Выполняем умножение (для проверки) ---
    переместить_имм рвх, 12
    переместить_имм рсх, 12
    умножить_знаковое рсх        ; рвх = 144

    ; --- Выводим строку "144\n" ---
    переместить_имм рвх, 1       ; sys_write
    переместить_имм рди, 1       ; stdout
    загрузить_адрес рис, текст
    переместить_имм рдх, 4       ; "144\n" = 4 байта
    вызов_системы

    ; --- Выходим с кодом 144 (из результата умножения) ---
    переместить рди, рвх         ; рди = 144
    переместить_имм рвх, 60      ; sys_exit
    вызов_системы