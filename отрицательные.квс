    .глобал _start
    .текст
    _start:
        переместить_имм рвх, 10          ; Загрузить 10 в рвх
        переместить_имм рсх, 25          ; Загрузить 25 в рсх
        вычесть рвх, рсх                 ; рвх = рвх - рсх → 10 - 25 = -15
        переместить рди, рвх             ; Подготовить аргумент exit: rdi = результат
        переместить_имм рвх, 60          ; Номер системного вызова exit = 60
        вызов_системы                    ; Вызвать ядро (завершить программу)

        ;./отрицательные.elf
        ;echo $?
