    .глобал _start
    .текст
    _start:
        ; Загрузить 10 в рвх
        переместить_имм рвх, 10
        ; Загрузить 25 в рсх
        переместить_имм рсх, 25
        ; Вычесть: рвх = рвх - рсх → 10 - 25 = -15
        вычесть рвх, рсх
        ; Теперь рвх = -15
        ; Подготовить системный вызов exit(rdi = рвх)
        переместить рди, рвх
        ; Номер системного вызова exit = 60
        переместить_имм рвх, 60
        ; Вызвать ядро
        вызов_системы

        ;./отрицательные.elf
        ;echo $?
